@startuml
entity Ticket {
    + id: UUID
    + nome_empresa: String
    + tipo: Tipo
    + criado_por: UUID
    + criado_em: DateTime
    + status: Status
    + prazo_conclusao: DateTime
    + urgencia_id: UUID
}

entity Tipo {
    * Abertura
    * Alteracao
    * Transformacao
    * Baixa
}

entity Status {
    * Aberto
    * Andamento
    * Concluido
}

entity Urgencia {
    + id: UUID
    + pq_da_urgencia: Text
    + anexar_comprovante: File
}

Ticket --> Tipo
Ticket --> Status
Ticket }o--|| Urgencia : "pode ter"
@enduml
